{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:@typescript-eslint/recommended",
        "prettier"
    ],
    "overrides": [],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module",
        "project": ["tsconfig.json", "lambdas/tsconfig.json"]
    },
    "rules": {
        "eol-last": ["error", "always"],
        "quotes": ["error", "single"],
        "semi": ["error", "always"],
        "no-new": 0
    }
}
